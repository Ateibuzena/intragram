  "graphTooltip": 0,
  "panels": [
    {
      "type": "stat",
      "title": "Example Service Up",
      "targets": [
        {
          "expr": "up{instance=\"example-service:3005\"}",
          "legendFormat": "up"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN"
                },
                "1": {
                  "text": "UP"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 6
      }
    },
    {
      "type": "timeseries",
      "title": "CPU Usage (user seconds/s)",
      "targets": [
        {
          "expr": "rate(process_cpu_user_seconds_total{instance=\"example-service:3005\"}[1m])",
          "legendFormat": "cpu user rate"
        }
      ],
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 18,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Memory RSS (MB)",
      "targets": [
        {
          "expr": "process_resident_memory_bytes{instance=\"example-service:3005\"} / 1024 / 1024",
          "legendFormat": "rss memory"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Event Loop Lag (p99)",
      "targets": [
        {
          "expr": "nodejs_eventloop_lag_p99_seconds{instance=\"example-service:3005\"}",
          "legendFormat": "lag p99"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      }
    }
  ],
